{
  "name": "nodejs-notes-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/app.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json --pretty",
    "inst_node": "npm i @types/node --save-dev; npm i node",
    "inst_tsdev": "npm i ts-node-dev --save-dev",
    "inst_yargs": "npm i @types/yargs --save-dev; npm i yargs",
    "install": "npm i chalk@4.1.2; npm run inst_node; npm run inst_tsdev; npm run inst_yargs"
  },
  "authors": "Joshua Loftin",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/yargs": "^17.0.10",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "node": "^17.7.2",
    "yargs": "^17.5.1"
  }
}
